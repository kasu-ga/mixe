{
  "name": "mixejs",
  "description": "Authentication in minutes for the server or client.",
  "keywords": [
    "authentication"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasu-ga/mixe.git"
  },
  "version": "1.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --minify terser",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "9.0.6",
    "@types/ms": "0.7.34"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "jose": "^5.4.1",
    "jsonwebtoken": "9.0.2",
    "ms": "2.1.3",
    "nanoid": "3.3.4"
  }
}
